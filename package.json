{
  "name": "tigger",
  "version": "1.0.0",
  "description": "Crypto trading bot with Telegram signal harvesting",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "docker:up": "docker compose up --build",
    "docker:build": "docker compose build",
    "evaluate": "tsx src/evaluation/index.ts",
    "evaluate:build": "npm run build && node dist/evaluation/index.js",
    "export-messages": "tsx src/scripts/export_messages.ts",
    "generate-session": "tsx src/scripts/generate_session.ts",
    "list-channels": "tsx src/scripts/list_channels/index.ts",
    "replay-message": "tsx src/scripts/replay_message.ts",
    "test-bybit-credentials": "tsx src/scripts/test_bybit_credentials.ts",
    "verify-parser": "tsx src/scripts/verify_parser.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "crypto",
    "trading",
    "bot",
    "telegram",
    "bybit"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "better-sqlite3": "^11.0.0",
    "bybit-api": "^4.4.2",
    "commander": "^7.2.0",
    "csv-parse": "^5.5.6",
    "csv-writer": "^1.6.0",
    "dayjs": "^1.11.19",
    "discord.js": "^14.14.1",
    "dotenv": "^17.2.3",
    "fast-redact": "^3.5.0",
    "fs-extra": "^11.3.2",
    "input": "^1.0.1",
    "pg": "^8.11.3",
    "telegram": "^2.26.22",
    "webpack-cli": "^4.10.0",
    "winston": "^3.15.0",
    "winston-loggly-bulk": "^3.3.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/fast-redact": "^3.0.4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.0",
    "@types/pg": "^8.10.9",
    "@types/winston-loggly-bulk": "^3.0.6",
    "@vitest/coverage-v8": "^4.0.14",
    "@vitest/ui": "^4.0.14",
    "tsx": "^4.16.0",
    "typescript": "^5.5.0",
    "vitest": "^4.0.14"
  }
}
