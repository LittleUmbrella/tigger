{
  "harvesters": [
    {
      "name": "main_harvester",
      "channel": "your_telegram_channel",
      "apiId": 12345678,
      "accessHash": "optional_access_hash_for_private_channels",
      "pollInterval": 5000
    }
  ],
  "parsers": [
    {
      "name": "main_parser",
      "channel": "your_telegram_channel",
      "ollama": {
        "baseUrl": "http://localhost:11434",
        "model": "llama3.2:1b",
        "timeout": 30000,
        "maxRetries": 2,
        "rateLimit": {
          "perChannel": 10,
          "perMinute": 30
        }
      }
    }
  ],
  "accounts": [
    {
      "name": "main",
      "exchange": "bybit",
      "testnet": false,
      "apiKey": "your_api_key_here",
      "apiSecret": "your_api_secret_here"
    },
    {
      "name": "testnet",
      "exchange": "bybit",
      "testnet": true,
      "apiKey": "your_testnet_api_key_here",
      "apiSecret": "your_testnet_api_secret_here"
    },
    {
      "name": "main_env",
      "exchange": "bybit",
      "testnet": false,
      "envVars": {
        "apiKey": "BYBIT_API_KEY",
        "apiSecret": "BYBIT_API_SECRET"
      }
    }
  ],
  "initiators": [
    {
      "name": "bybit",
      "riskPercentage": 3,
      "accounts": ["main"]
    },
    {
      "name": "bybit_multi",
      "riskPercentage": 3,
      "accounts": ["main", "testnet"]
    }
  ],
  "monitors": [
    {
      "type": "bybit",
      "testnet": false,
      "pollInterval": 10000,
      "entryTimeoutDays": 2
    }
  ],
  "channels": [
    {
      "channel": "your_telegram_channel",
      "harvester": "main_harvester",
      "parser": "main_parser",
      "initiator": "bybit",
      "monitor": "bybit"
    }
  ],
  "database": {
    "type": "sqlite",
    "path": "data/trading_bot.db"
  }
}
