{
  "_comment": "This config file is safe to commit to version control. Database connection string comes from DATABASE_URL environment variable. Sensitive values (accessHash, botToken, API keys) use envVarNames to reference environment variables.",
  "harvesters": [
    {
      "name": "main_harvester",
      "channel": "your_telegram_channel",
      "envVarNames": {
        "apiId": "TG_API_ID",
        "accessHash": "TELEGRAM_ACCESS_HASH"
      },
      "pollInterval": 5000,
      "downloadImages": false
    }
  ],
  "parsers": [
    {
      "name": "main_parser",
      "channel": "your_telegram_channel"
    }
  ],
  "initiators": [
    {
      "name": "bybit",
      "testnet": false,
      "riskPercentage": 3
    }
  ],
  "monitors": [
    {
      "type": "bybit",
      "testnet": false,
      "pollInterval": 10000,
      "entryTimeoutMinutes": 2880 // 2 days = 2880 minutes
    }
  ],
  "channels": [
    {
      "channel": "your_telegram_channel",
      "harvester": "main_harvester",
      "parser": "main_parser",
      "initiator": "bybit",
      "monitor": "bybit"
    }
  ],
  "database": {
    "type": "postgresql"
  }
}


