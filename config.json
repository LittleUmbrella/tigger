{
  "_comment": "This config file is safe to commit to version control. Database connection string comes from DATABASE_URL environment variable.",
  "harvesters": [
    {
      "name": "main_harvester",
      "channel": "2427485240",
      "envVarNames": {
        "apiId": "TG_API_ID",
        "accessHash": "TG_ACCESS_HASH"
      },
      "pollInterval": 3000,
      "downloadImages": false,
      "skipOldMessagesOnStartup": true,
      "maxMessageAgeMinutes": 5
    },
    {
      "name": "ronnie_harvester",
      "channel": "3241720654",
      "envVarNames": {
        "apiId": "TG_API_ID",
        "accessHash": "TG_ACCESS_HASH"
      },
      "pollInterval": 3000,
      "downloadImages": false,
      "skipOldMessagesOnStartup": true,
      "maxMessageAgeMinutes": 5
    }
  ],
  "parsers": [
    {
      "name": "vip_crypto_signals",
      "channel": "2427485240"
    },
    {
      "name": "ronnie_crypto_signals",
      "channel": "3241720654"
    }
  ],
  "accounts": [
    {
      "name": "testnet2",
      "exchange": "bybit",
      "testnet": true,
      "envVarNames": {
        "apiKey": "BYBIT_TESTNET_API_KEY2",
        "apiSecret": "BYBIT_TESTNET_API_SECRET2"
      }
    },
    {
      "name": "testnet",
      "exchange": "bybit",
      "testnet": true,
      "envVarNames": {
        "apiKey": "BYBIT_TESTNET_API_KEY",
        "apiSecret": "BYBIT_TESTNET_API_SECRET"
      }
    }
  ],
  "initiators": [
    {
      "name": "bybit",
      "testnet": true,
      "riskPercentage": 1,
      "accounts": ["testnet", "testnet2"]
    }
  ],
  "monitors": [
    {
      "type": "bybit",
      "testnet": true,
      "pollInterval": 3000,
      "entryTimeoutDays": 1
    }
  ],
  "channels": [
    {
      "channel": "vip-future",
      "harvester": "main_harvester",
      "parser": "vip_crypto_signals",
      "breakevenAfterTPs": 2,
      "initiator": "bybit",
      "monitor": "bybit"
    },
    {
      "channel": "ronnie-crypto-signals",
      "harvester": "ronnie_harvester",
      "parser": "ronnie_crypto_signals",
      "breakevenAfterTPs": 2,
      "initiator": "bybit",
      "monitor": "bybit"
    }
  ],
  "database": {
    "type": "postgresql"
  }
}


