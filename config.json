      "testnet": true,
      "envVarNames": {
        "apiKey": "BYBIT_TESTNET_API_KEY2",
        "apiSecret": "BYBIT_TESTNET_API_SECRET2"
      }
    },
    {
      "name": "testnet",
      "exchange": "bybit",
      "testnet": true,
      "envVarNames": {
        "apiKey": "BYBIT_TESTNET_API_KEY",
        "apiSecret": "BYBIT_TESTNET_API_SECRET"
